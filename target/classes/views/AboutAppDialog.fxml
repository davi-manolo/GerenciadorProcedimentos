<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox maxWidth="315.0" minWidth="315.0" prefWidth="315.0" styleClass="appBody" stylesheets="@../styles/manager.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.AboutAppDialog">
   <children>
      <VBox alignment="TOP_CENTER" spacing="15.0" styleClass="appWindow" VBox.vgrow="ALWAYS">
         <children>
            <ImageView fitHeight="55.0" fitWidth="55.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/icon.png" />
               </image>
               <VBox.margin>
                  <Insets top="40.0" />
               </VBox.margin>
            </ImageView>
            <Label fx:id="nameLabel" text="Name Application" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" spacing="3.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="VersÃ£o do App:">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Text>
                  <Label fx:id="versionLabel" text="0.0.0">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets top="-10.0" />
               </VBox.margin>
            </HBox>
            <Label fx:id="descriptionLabel" text="Description here..." textAlignment="CENTER" wrapText="true" VBox.vgrow="ALWAYS">
               <font>
                  <Font size="11.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <Label text="Desenvolvido por Davi Manolo" textAlignment="CENTER">
               <font>
                  <Font size="11.0" />
               </font>
               <VBox.margin>
                  <Insets top="-10.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="closeAppButton" minHeight="28.0" mnemonicParsing="false" onAction="#closeButtonAction" prefHeight="28.0" prefWidth="240.0" styleClass="buttonManager" text="OK" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</VBox>
